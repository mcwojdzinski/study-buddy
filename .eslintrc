{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warning", {
            "singleQuote": true,
            "printWidth": 150
        }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@babel/eslint-parser"
}